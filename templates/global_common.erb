#
# this file is managed by puppet!
#

global {
<%- global.each do |key,value| -%>
	<%= key %>	<%= value %>;
<%- end -%>

common {
<%- common.each do |key,value| -%>
<%- if value.is_a?(Hash) -%>
	<%= key %> {
<%- value.each do |k,v| -%>
		<%= k %>	<%= v %>;
<%- end -%>
	}
<%- else -%>
	<%= key %>	<%= value %>;
<%- end -%>
<%- end -%>
}
